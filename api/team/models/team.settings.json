{
  "connection": "default",
  "collectionName": "teams",
  "info": {
    "name": "Team"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "name": {
      "type": "string",
      "private": false,
      "required": true,
    },
    "logo": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": true
    },
    "website": {
      "type": "string"
    },
    "external_roster": {
      "type": "string"
    },
    "games": {
      "collection": "game",
      "via": "visitor"
    },
    "roster": {
      "collection": "player",
      "via": "team"
    },
    "standing": {
      "via": "team",
      "model": "standing"
    },
    "slug": {
      "type": "string"
    }
  }
}
