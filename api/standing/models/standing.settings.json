{
  "connection": "default",
  "collectionName": "standings",
  "info": {
    "name": "Standing"
  },
  "options": {
    "increments": true,
    "timestamps": true
  },
  "attributes": {
    "team": {
      "model": "team",
      "via": "standing"
    },
    "season": {
      "type": "enumeration",
      "enum": [
        "spring",
        "summer",
        "fall"
      ],
      "default": "spring",
      "required": true
    },
    "GP": {
      "type": "integer",
      "default": 0,
      "min": 0
    },
    "W": {
      "type": "integer",
      "default": 0
    },
    "D": {
      "type": "integer",
      "default": 0
    },
    "L": {
      "type": "integer",
      "default": 0
    },
    "GF": {
      "type": "integer",
      "default": 0
    },
    "GA": {
      "type": "integer",
      "default": 0
    },
    "GD": {
      "type": "integer",
      "default": 0
    },
    "PT": {
      "type": "integer",
      "default": 0
    },
    "teamName": {
      "type": "string"
    },
    "Latest": {
      "type": "enumeration",
      "enum": [
        "W",
        "T"
        "L",
      ],
      "required": false
    },
    "HomeWTL": {
      "type": "component",
      "repeatable": false,
      "component": "w-t-l.w-l-d"
    },
    "AwayWTL": {
      "type": "component",
      "repeatable": false,
      "component": "w-t-l.w-l-d"
    }
  }
}
